schema currency {
    document currency {
        field idx type byte {
            indexing: summary | attribute
            attribute: fast-search
            rank: filter
        }
        field code type string {
            indexing: attribute | summary
            attribute: fast-search
        }
        field factor type double {
            indexing: attribute | summary
            attribute: fast-search
            rank: filter
        }
        field factor_map type map<string, double> {
            indexing: summary
        }
    }
}
